{
  "name": "@hmin/js-toolkit",
  "description": "Lightweight modern JavaScript utility library",
  "version": "0.0.2",
  "keywords": [
    "javascript",
    "typescript",
    "utility"
  ],
  "author": "hmin",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmin01/jstoolkit.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && tsc --noEmit && vite build --sourcemap false",
    "clean": "rimraf dist",
    "prepack": "yarn test && yarn build",
    "preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "vitest run --coverage --typecheck"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "4.0.7",
    "glob": "^11.0.3",
    "path": "^0.12.7",
    "rimraf": "^6.1.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.7"
  },
  "dependencies": {
    "@types/node": "^24.10.0",
    "esbuild": "^0.25.12",
    "nanoid": "^3.3.11",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.3",
    "postcss": "^8.5.6",
    "rollup": "^4.52.5",
    "source-map-js": "^1.2.1",
    "tinyglobby": "^0.2.15"
  }
}
